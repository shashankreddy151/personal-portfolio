{
  "name": "personal-site",
  "version": "4.0.0",
  "homepage": "https://mldangelo.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mldangelo/personal-site.git"
  },
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer build/static/js/*.chunk.js",
    "build": "rimraf ./build && react-scripts build",
    "lint": "eslint \"**/*.js\"",
    "predeploy": "npm run build && react-snap",
    "start": "react-scripts start",
    "test": "npx jest"
  },
  "dependencies": {
    "gh-pages": "^6.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
